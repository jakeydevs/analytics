{
  "name": "jakeydevs/analytics",
  "description": "A self-hosted analytic plugin for Laravel",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "jakey",
      "email": "jp@jakelprice.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=7.2",
    "laravel/laravel": "8.*",
    "whichbrowser/parser": "dev-master",
    "geoip2/geoip2": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Jakeydevs\\Analytics\\": "src",
      "Jakeydevs\\Analytics\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Jakeydevs\\Analytics\\AnalyticServiceProvider"
      ]
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5@dev",
    "orchestra/testbench": "^7.0@dev"
  },
  "scripts": {
    "test": "phpunit"
  }
}
