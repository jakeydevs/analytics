{
  "name": "jakeydevs/analytics",
  "description": "A self-hosted analytic system for Laravel",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "jakey",
      "email": "jp@jakelprice.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "whichbrowser/parser": "^2.0",
    "geoip2/geoip2": "^2.11"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.4",
    "orchestra/testbench": "^6.2"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Jakeydevs\\Analytics\\": "src",
      "Jakeydevs\\Analytics\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Jakeydevs\\Analytics\\AnalyticServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "phpunit"
  }
}
